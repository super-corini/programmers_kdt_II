   "source": [
    "### L2 norm을 구하는 함수"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_L2_norm(arr):\n",
    "    return np.sqrt(np.sum(arr @ arr[:, None]))"
   ]
  },
